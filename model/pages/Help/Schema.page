=ChangeLog.xsd=
<pre>
<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	xmlns:wat="http://tide.act.nato.int/team/wat"
	targetNamespace="http://tide.act.nato.int/team/wat"
	elementFormDefault="qualified">

	<xsd:complexType name="ChangeLog">
		<xsd:annotation>
			<xsd:documentation>The collection of all Wiki Model versions.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" name="version" type="wat:Version"/>
		</xsd:sequence>
	</xsd:complexType>


	<xsd:complexType name="Version">
		<xsd:annotation>
			<xsd:documentation>Defines a single version of a Wiki Model.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="name" type="xsd:decimal" use="required"/>
				<xsd:attribute name="date" type="xsd:date" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>

</xsd:schema>

</pre>

