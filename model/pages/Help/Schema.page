* '''DataModel.xsd''' defines elements to model domain specific data entities (Packages, Concepts, Properties)
** '''ConceptMapping.xsd''' defines elements which allow to map Concepts to concepts known to Wiki Automation Tool (e.g. Period)
** '''Columns.xsd''' defines elements to style display of the Concepts on the pages
** '''Widgets.xsd''' defined other custom widgets which might be used on the pages (e.g. Navigator)
* '''ReportsModel.xsd''' defines elements to describe reports which are being generated
* '''ChangeLog.xsd''' defines elements to describe various versions of the Wiki Models
* '''Utility.xsd''' defines general use types used across all other schemas


<div style="clear:both"></div>

=WikiModel.xsd=
<pre>
<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:wat="http://tide.act.nato.int/team/wat"
	targetNamespace="http://tide.act.nato.int/team/wat"
	elementFormDefault="qualified">

	<!-- include component definitions -->
	<xsd:include schemaLocation="DataModel.xsd"/>
	<xsd:include schemaLocation="ReportsModel.xsd"/>
	<xsd:include schemaLocation="ChangeLog.xsd"/>

	<xsd:element name="WikiModel" type="wat:WikiModel"/>

	<xsd:complexType name="WikiModel">
		<xsd:annotation>
			<xsd:documentation>The root element of the Wiki Model.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="description" type="xsd:string"/>
			<xsd:element name="header" minOccurs="0"/>
			<xsd:element name="ChangeLog" type="wat:ChangeLog"/>
			<xsd:element name="DataModel" type="wat:DataModel"/>
			<xsd:element name="ReportsModel" type="wat:ReportsModel" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="name" type="xsd:string" use="required"/>
		<xsd:attribute name="image" type="xsd:string" use="required"/>
		<xsd:attribute name="theme">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="mars"/>
					<xsd:enumeration value="pluton"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="issueTracker" type="xsd:string"/>
		<xsd:attribute name="gitRepository" type="xsd:string"/>
	</xsd:complexType>

</xsd:schema>

</pre>

=DataModel.xsd=
<pre>
<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:wat="http://tide.act.nato.int/team/wat"
	targetNamespace="http://tide.act.nato.int/team/wat"
	elementFormDefault="qualified">

	<xsd:include schemaLocation="Utility.xsd"/>
	<xsd:include schemaLocation="Layout.xsd"/>
	<xsd:include schemaLocation="Columns.xsd"/>
	<xsd:include schemaLocation="ConceptMapping.xsd"/>
	<xsd:include schemaLocation="Widgets.xsd"/>
	<xsd:include schemaLocation="Security.xsd"/>


	<xsd:complexType name="DataModel">
		<xsd:sequence>
			<xsd:element name="import" type="wat:ImportedModel" maxOccurs="unbounded" />
			<xsd:element name="concepts">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="package" type="wat:Package" maxOccurs="unbounded" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="properties">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="property" type="wat:PropertyDefinition" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>


	<xsd:complexType name="ImportedModel">
		<xsd:annotation>
			<xsd:documentation>Represents an external model which can be included.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="model" type="xsd:string" use="required"/>
	</xsd:complexType>


	<xsd:complexType name="Package">
		<xsd:annotation>
			<xsd:documentation>Represents a group of related Concepts.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="description" type="xsd:string"/>
			<xsd:element name="concept" minOccurs="0" type="wat:Concept" maxOccurs="unbounded"/>
			<xsd:element name="reports" type="wat:Reports" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="navigator" type="wat:Navigator" minOccurs="0" />
		</xsd:sequence>
		<xsd:attribute name="name" type="xsd:string" use="required"/>
	</xsd:complexType>


	<xsd:complexType name="Concept">
		<xsd:annotation>
			<xsd:documentation>Represents single Concept</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="description" type="xsd:string"/>

			<!-- Page Naming Extension -->
			<xsd:element name="nomenclature" minOccurs="0">
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="xsd:string">
							<xsd:attribute name="autopage" type="xsd:string"/>
							<xsd:attribute name="namescheme" type="xsd:string"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Core Concept Information -->
			<xsd:element name="structure">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="wat:field" minOccurs="0" maxOccurs="unbounded"/>
						<xsd:element name="customView" minOccurs="0" maxOccurs="1"/>
					</xsd:sequence>

					<!-- Layout configuration -->
					<xsd:attribute name="layout" type="wat:LayoutType"/>
					<xsd:attribute name="formLayout" type="wat:LayoutType"/>
					<xsd:attribute name="gridColumns" type="xsd:integer"/>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="conceptMapping" type="wat:ConceptMapping" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="reports" type="wat:Reports" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="security" type="wat:ConceptSecurity" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="dashboards" type="wat:Dashboards" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="relatedData" type="wat:RelatedData" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>

		<!-- Core Concept Information -->
		<xsd:attribute name="name" type="xsd:string" use="required"/>
		<xsd:attribute name="plural" type="xsd:string" use="required"/>
		<xsd:attribute name="type" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="Static"/>
					<xsd:enumeration value="Dynamic"/>
					<xsd:enumeration value="Virtual"/>
					<xsd:enumeration value="Internal"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="acronym" type="xsd:string"/>
		<xsd:attribute name="image" type="xsd:string"/>
		<xsd:attribute name="color" type="xsd:string"/>
		<xsd:attribute name="protected" type="xsd:boolean"/>

		<!-- Concept Inheritance Extension -->
		<xsd:attribute name="parent" type="xsd:string"/>

		<!-- Hierarchical Data Extension -->
		<xsd:attribute name="hierarchy">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="tree"/>
					<xsd:enumeration value="forest"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="forestProperty" type="xsd:string"/>
		<xsd:attribute name="topic" type="xsd:string"/>

		<!-- Data Sorting Extension -->
		<xsd:attribute name="ordered" type="xsd:boolean"/>

		<!-- Data Security Extension -->
		<xsd:attribute name="createPermission" type="xsd:string"/> <!--the value is User Group-->
	</xsd:complexType>


	<xsd:element name="field" type="wat:Field" abstract="true" />
	<xsd:complexType name="Field" abstract="true">
		<xsd:sequence>
			<xsd:element name="customView" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>

		<xsd:attribute name="name" type="xsd:string" use="required"/>

		<!-- Conditional View and Edit Extension -->
		<xsd:attribute name="viewable" type="xsd:boolean"/>
		<xsd:attribute name="viewableWhen" type="xsd:string"/>
		<xsd:attribute name="viewableInState" type="xsd:string"/>
		<xsd:attribute name="viewPowerGroup" type="xsd:string"/>
		<xsd:attribute name="editable" type="xsd:boolean"/>
		<xsd:attribute name="editableWhen" type="xsd:string"/>
		<xsd:attribute name="editableInState" type="xsd:string"/>
		<xsd:attribute name="editPowerGroup" type="xsd:string"/>

		<!-- Layout Extension -->
		<xsd:attribute name="gridRow" type="xsd:integer"/>
		<xsd:attribute name="gridRowSpan" type="xsd:integer"/>
		<xsd:attribute name="gridColumn" type="xsd:integer"/>
		<xsd:attribute name="gridColumnSpan" type="xsd:integer"/>

	</xsd:complexType>


	<xsd:element name="property" type="wat:PropertyField" substitutionGroup="wat:field" />
	<xsd:complexType name="PropertyField">
		<xsd:annotation>
			<xsd:documentation>Defines a Concept Property/Attribute. It is a link to the property definition in properties collection.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="wat:Field">
				<xsd:sequence>
					<!-- Page Property Information -->
					<xsd:element name="columns" minOccurs="0" maxOccurs="1" type="wat:ColumnList"/>
					<xsd:element name="relatedData" minOccurs="0" maxOccurs="1" type="wat:PropertyRelatedData"/>
				</xsd:sequence>

				<!-- Core Property Information -->
				<xsd:attribute name="label" type="xsd:string"/>
				<xsd:attribute name="attribute" type="xsd:string"/>
				<xsd:attribute name="default" type="xsd:string"/>
				<xsd:attribute name="inputValue" type="xsd:string"/>
				<xsd:attribute name="displayValue" type="xsd:string"/>
				<xsd:attribute name="propertyValue" type="xsd:string"/>
				<xsd:attribute name="mandatory" type="xsd:boolean"/>
				<xsd:attribute name="multiplicity">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="one"/>
							<xsd:enumeration value="multiple"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>

				<!-- Common Property Information -->
				<xsd:attribute name="format">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">

							<!-- Page Property formats -->
							<xsd:enumeration value="table"/>
							<xsd:enumeration value="list"/>
							<xsd:enumeration value="link"/>
							<xsd:enumeration value="gallery"/>
							<xsd:enumeration value="gallerylist"/>

							<!-- for Option Property Type -->
							<xsd:enumeration value="bullets"/>
							<xsd:enumeration value="commas"/>

							<!-- for Query Property Type -->
							<xsd:enumeration value="tree"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>

				<xsd:attribute name="editField">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">

							<!-- Option edit fields -->
							<!-- to select multiple values -->
							<xsd:enumeration value="listbox"/>
							<xsd:enumeration value="checkboxes"/>
							<!-- to select single value -->
							<xsd:enumeration value="combobox"/>
							<xsd:enumeration value="radiobutton"/>

						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>

				<!-- Page Property Information -->
				<xsd:attribute name="refersTo" type="xsd:string"/>
				<xsd:attribute name="checkIfExists" type="xsd:string"/>
				<xsd:attribute name="stereotype">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="association"/>
							<xsd:enumeration value="aggregation"/>
							<xsd:enumeration value="composition"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="filter" type="xsd:string"/>
				<xsd:attribute name="existingonly" type="xsd:boolean"/>
				<xsd:attribute name="icon" type="xsd:string"/>

				<!-- Image Property Information -->
				<xsd:attribute name="width" type="xsd:integer"/>

			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:element name="subobject" type="wat:SubobjectField" substitutionGroup="wat:field" />
	<xsd:complexType name="SubobjectField">
		<xsd:complexContent>
			<xsd:extension base="wat:Field">
				<xsd:attribute name="concept" type="xsd:string" use="required"/>
				<xsd:attribute name="attribute" type="xsd:string" use="required"/>
				<xsd:attribute name="label" type="xsd:string"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="PropertyRelatedData">
		<xsd:sequence>
			<xsd:element name="columns" minOccurs="0" maxOccurs="1" type="wat:ColumnList"/>
		</xsd:sequence>
		<xsd:attribute name="format">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="table"/>
					<xsd:enumeration value="list"/>
					<xsd:enumeration value="link"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>


	<xsd:complexType name="PropertyDefinition">
		<xsd:sequence>
			<!-- Core Property Definition -->
			<xsd:element name="description" type="xsd:string"/>
			<xsd:element name="instruction" type="xsd:string" minOccurs="0"/>

			<!-- Option Type Extension -->
			<xsd:element name="values" type="wat:Values" minOccurs="0"/>

			<!-- Workflow Type Extension -->
			<xsd:element name="states" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="state" type="wat:WorkflowState" minOccurs="2" maxOccurs="unbounded"/>
					</xsd:sequence>
					<xsd:attribute name="start" type="xsd:string" use="required"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>

		<!-- Core Property Definition -->
		<xsd:attribute name="name" type="xsd:string" use="required"/>
		<xsd:attribute name="type" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="Boolean"/>
					<xsd:enumeration value="Bugzilla"/>
					<xsd:enumeration value="Date"/>
					<xsd:enumeration value="DateTime"/>
					<xsd:enumeration value="Email"/>
					<xsd:enumeration value="File"/>
					<xsd:enumeration value="Geographic coordinates"/>
					<xsd:enumeration value="Image"/>
					<xsd:enumeration value="Number"/>
					<xsd:enumeration value="Option"/>
					<xsd:enumeration value="Page"/>
					<xsd:enumeration value="String"/>
					<xsd:enumeration value="Text"/>
					<xsd:enumeration value="Time"/>
					<xsd:enumeration value="URL"/>
					<xsd:enumeration value="Workflow"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="label" type="xsd:string" use="required"/>
		<xsd:attribute name="attribute" type="xsd:string" use="required"/>

		<!-- Booelan Property Extension -->
		<xsd:attribute name="labelTrue" type="xsd:string"/>
		<xsd:attribute name="labelFalse" type="xsd:string"/>

		<!-- Page Property Extension -->
		<xsd:attribute name="refersTo" type="xsd:string"/>

		<!-- Edit Field Extension -->
		<xsd:attribute name="size" type="xsd:integer"/>
		<xsd:attribute name="rows" type="xsd:integer"/>

		<!-- Image Property Information -->
		<xsd:attribute name="width" type="xsd:integer"/>

	</xsd:complexType>

	<xsd:complexType name="WorkflowState">
		<xsd:sequence>
			<xsd:element name="description" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="state" type="wat:WorkflowState" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="transition" type="wat:StateTransition" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="name" type="xsd:string" use="required"/>
		<xsd:attribute name="label" type="xsd:string"/>
		<xsd:attribute name="color" type="xsd:string"/>
	</xsd:complexType>

	<xsd:complexType name="StateTransition">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="name" type="xsd:string" use="required"/>
				<xsd:attribute name="label" type="xsd:string"/>
				<xsd:attribute name="to" type="xsd:string" use="required"/>
				<xsd:attribute name="icon" type="xsd:string" use="required"/>
				<xsd:attribute name="userGroup" type="xsd:string"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="Values">
		<xsd:annotation>
			<xsd:documentation>The container to define the list of values (enumeration).</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="value" type="wat:Value" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="default" type="xsd:string"/>
	</xsd:complexType>

	<xsd:complexType name="Value">
		<xsd:annotation>
			<xsd:documentation>The element to define an allowable value in enumeration (values)</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="name" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>

</xsd:schema>

</pre>

=ConceptMapping.xsd=
<pre>
<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:wat="http://tide.act.nato.int/team/wat"
	targetNamespace="http://tide.act.nato.int/team/wat"
	elementFormDefault="qualified">


	<xsd:complexType name="ConceptMapping">
		<xsd:annotation>
			<xsd:documentation>Allows to map the properties of one Concept to the properties of another one.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" maxOccurs="unbounded" name="propertyMapping" type="wat:PropertyMapping"/>
			<xsd:element minOccurs="0" maxOccurs="unbounded" name="subobjectMapping" type="wat:SubobjectMapping"/>
		</xsd:sequence>
		<xsd:attribute name="name" type="xsd:string" use="required"/>
		<xsd:attribute name="function" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="GanttChart"/>
					<xsd:enumeration value="DeriveData"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>

	<xsd:complexType name="SubobjectMapping">
		<xsd:annotation>
			<xsd:documentation>Represents a mapping of one subobject to another.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" name="propertyMapping" type="wat:PropertyMapping"/>
		</xsd:sequence>
		<xsd:attribute name="name" type="xsd:string" use="required"/>
		<xsd:attribute name="subobject" type="xsd:string"/>
	</xsd:complexType>

	<xsd:complexType name="PropertyMapping">
		<xsd:annotation>
			<xsd:documentation>Represents a mapping of one property to another one.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="name" type="xsd:string" use="required"/>
		<xsd:attribute name="property" type="xsd:string"/>
		<xsd:attribute name="refersTo" type="xsd:string"/>
		<xsd:attribute name="value" type="xsd:string"/>
	</xsd:complexType>

</xsd:schema>

</pre>

=Columns.xsd=
<pre>
<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:wat="http://tide.act.nato.int/team/wat"
	targetNamespace="http://tide.act.nato.int/team/wat"
	elementFormDefault="qualified">

	<xsd:complexType name="ColumnList">
		<xsd:sequence>
			<xsd:element name="column" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:simpleContent>
	 					<xsd:extension base="xsd:string">
							<xsd:attribute name="name" type="xsd:string" use="required"/>
	 					</xsd:extension>
 					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="countColumn" type="wat:CountColumn" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CountColumn">
		<xsd:annotation>
			<xsd:documentation>Definition of column to display a count of records</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="name"     type="xsd:string"  use="required"/>
		<xsd:attribute name="label"    type="xsd:string"/>
		<xsd:attribute name="concept"  type="xsd:string"/>
		<xsd:attribute name="property" type="xsd:string"/>
		<xsd:attribute name="filter"   type="xsd:string"/>
		<xsd:attribute name="custom"   type="xsd:string"/>
	</xsd:complexType>

</xsd:schema>

</pre>

=Widgets.xsd=
<pre>
<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:wat="http://tide.act.nato.int/team/wat"
	targetNamespace="http://tide.act.nato.int/team/wat"
	elementFormDefault="qualified">


	<xsd:element name="chart" type="wat:ChartField" substitutionGroup="wat:field" />
	<xsd:complexType name="ChartField">
		<xsd:complexContent>
			<xsd:extension base="wat:Field">
				<xsd:sequence>
					<xsd:element name="colors" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="color" maxOccurs="unbounded">
									<xsd:complexType>
										<xsd:simpleContent>
			 								<xsd:extension base="xsd:string">
												<xsd:attribute name="value" type="xsd:string" use="required"/>
			 								</xsd:extension>
		 								</xsd:simpleContent>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="label" type="xsd:string"/>
				<xsd:attribute name="concept" type="xsd:string" use="required"/>
				<xsd:attribute name="subobject" type="xsd:string"/>
				<xsd:attribute name="groupBy" type="xsd:string" use="required"/>
				<xsd:attribute name="filter" type="xsd:string"/>
				<xsd:attribute name="type" use="required">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="pie"/>
							<xsd:enumeration value="bar"/>
							<xsd:enumeration value="line"/>
							<xsd:enumeration value="donut"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="legendLocation">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="n"/>
							<xsd:enumeration value="ne"/>
							<xsd:enumeration value="nw"/>
							<xsd:enumeration value="s"/>
							<xsd:enumeration value="se"/>
							<xsd:enumeration value="sw"/>
							<xsd:enumeration value="e"/>
							<xsd:enumeration value="w"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="width" type="xsd:integer"/>
				<xsd:attribute name="height" type="xsd:integer"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:element name="group" type="wat:GroupField" substitutionGroup="wat:field" />
	<xsd:complexType name="GroupField">
		<xsd:complexContent>
			<xsd:extension base="wat:Field">
				<xsd:sequence>
					<xsd:element ref="wat:field" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
				<xsd:attribute name="label" type="xsd:string"/>

				<!-- Layout configuration -->
				<xsd:attribute name="layout" type="wat:LayoutType"/>
				<xsd:attribute name="formLayout" type="wat:LayoutType"/>
				<xsd:attribute name="gridColumns" type="xsd:integer"/>
				<xsd:attribute name="bannerLeft" type="xsd:string"/>
				<xsd:attribute name="bannerRight" type="xsd:string"/>
				<xsd:attribute name="bannerCenter" type="xsd:string"/>
				<xsd:attribute name="bannerBottom" type="xsd:string"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:element name="userQuery" type="wat:UserQueryField" substitutionGroup="wat:field" />
	<xsd:complexType name="UserQueryField">
		<xsd:complexContent>
			<xsd:extension base="wat:Field">
				<xsd:sequence>
					<xsd:element name="columns" minOccurs="0" maxOccurs="1" type="wat:ColumnList"/>
				</xsd:sequence>
				<xsd:attribute name="concept" type="xsd:string" use="required"/>
				<xsd:attribute name="label" type="xsd:string"/>
				<xsd:attribute name="new" type="xsd:boolean"/>
				<xsd:attribute name="format">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="table"/>
							<xsd:enumeration value="tree"/>
							<xsd:enumeration value="list"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:element name="query" type="wat:QueryField" substitutionGroup="wat:field" />
	<xsd:complexType name="QueryField">
		<xsd:complexContent>
			<xsd:extension base="wat:Field">
				<xsd:sequence>
					<xsd:element name="columns" minOccurs="0" maxOccurs="1" type="wat:ColumnList"/>
				</xsd:sequence>
				<xsd:attribute name="concept" type="xsd:string"/>
				<xsd:attribute name="subobject" type="xsd:string"/>
				<xsd:attribute name="label" type="xsd:string"/>
				<xsd:attribute name="filter" type="xsd:string"/>
				<xsd:attribute name="sortBy" type="xsd:string"/>
				<xsd:attribute name="sortOrder">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="asc"/>
							<xsd:enumeration value="desc"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="format">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="table"/>
							<xsd:enumeration value="list"/>
							<xsd:enumeration value="link"/>
							<xsd:enumeration value="gallery"/>
							<xsd:enumeration value="gallerylist"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="icon" type="xsd:string"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:element name="reference" type="wat:ReferenceField" substitutionGroup="wat:field" />
	<xsd:complexType name="ReferenceField">
		<xsd:complexContent>
			<xsd:extension base="wat:QueryField">
				<xsd:attribute name="refersFrom" type="xsd:string" use="required"/>
				<xsd:attribute name="property" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:element name="referenceGroups" type="wat:ReferenceGroupsField" substitutionGroup="wat:field" />
	<xsd:complexType name="ReferenceGroupsField">
		<xsd:complexContent>
			<xsd:extension base="wat:ReferenceField">
				<xsd:attribute name="groupConcept" type="xsd:string" use="required"/>
				<xsd:attribute name="groupBy" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:element name="propertyList" type="wat:PropertyListField" substitutionGroup="wat:field" />
	<xsd:complexType name="PropertyListField">
		<xsd:complexContent>
			<xsd:extension base="wat:QueryField">
				<xsd:attribute name="property" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:element name="propertyGroups" type="wat:PropertyGroupsField" substitutionGroup="wat:field" />
	<xsd:complexType name="PropertyGroupsField">
		<xsd:complexContent>
			<xsd:extension base="wat:PropertyListField">
				<xsd:attribute name="groupConcept" type="xsd:string" use="required"/>
				<xsd:attribute name="groupBy" type="xsd:string" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:element name="staticContent" type="wat:StaticContentField" substitutionGroup="wat:field" />
	<xsd:complexType name="StaticContentField">
		<xsd:complexContent>
			<xsd:extension base="wat:Field">
				<xsd:sequence>
					<xsd:element name="text" type="xsd:string"/>
				</xsd:sequence>
				<xsd:attribute name="label" type="xsd:string"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="Reports">
		<xsd:annotation>
			<xsd:documentation>The container to define the list of values (enumeration).</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="report" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="annex" minOccurs="0" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:attribute name="name" type="xsd:string" use="required"/>
								<xsd:attribute name="concept" type="xsd:string" use="required"/>
								<xsd:attribute name="property" type="xsd:string" use="required"/>
								<xsd:attribute name="file" type="xsd:string" use="required"/>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
					<xsd:attribute name="name" type="xsd:string" use="required"/>
					<xsd:attribute name="category" type="xsd:string" use="required"/>
					<xsd:attribute name="file" type="xsd:string" use="required"/>
					<xsd:attribute name="scope">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="package"/>
								<xsd:enumeration value="concept"/>
								<xsd:enumeration value="instance"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="navigator" type="wat:Navigator" substitutionGroup="wat:field" />
	<xsd:complexType name="Navigator">
		<xsd:complexContent>
			<xsd:extension base="wat:Field">
				<xsd:attribute name="label" type="xsd:string"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="Dashboards">
		<xsd:sequence>
			<xsd:element name="dashboard" type="wat:Dashboard" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="Dashboard">
		<xsd:complexContent>
			<xsd:extension base="wat:GroupField">
				<xsd:attribute name="icon" type="xsd:string" use="required"/>
				<xsd:attribute name="printableView" type="xsd:boolean"/>
				<xsd:attribute name="context" type="xsd:string"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="RelatedData">
		<xsd:sequence>
			<xsd:element name="hide" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:attribute name="concept" type="xsd:string" use="required"/>
					<xsd:attribute name="property" type="xsd:string" use="required"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="hide" type="xsd:boolean"/>
	</xsd:complexType>

</xsd:schema>

</pre>

